@model WebApplication1.Areas.Security.Models.UserView

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<header class="row tab-content-header">
    <div class="col-md-6">

        <h3>User Detail</h3>

    </div>

    <p>
        <div class="col-md-6 text-right">

            <a href="@Url.Action("Index")" class="">
                <span class="glyphicon glyphicon-arrow-left"></span>
                Back to list
            </a>
        </div>
    </p>

</header>

<hr/>


<div class="panel-body">
   
    <img src="@Url.Content("~/Pictures/Unknown.jpg")" class="img-rounded resize" />
    <p>
        @Html.ActionLink("Edit", "Edit", new { id = Model.Id }) 

    </p>
    <p class="text">
        <b>
            @Html.DisplayFor(model => model.LastName)
        </b>
    </p>
    @Html.DisplayFor(model => model.FirstName)
    <br><br />
    @Html.DisplayFor(model => model.Age) years old
    <br><br />
    @Html.DisplayFor(model => model.Gender)
</div>
    </div>

